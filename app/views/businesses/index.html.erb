<h1> ALL businesses <h1>
<table>
    <thead>
        <tr>
            <td>Business Name</td>
            <td> Job Title</td>
            <td> Job Pay</td>
        <tr>
    </thead>
    <tbody>
    <%@businesses.each do |business|%>
    <tr class="business-row" id="<%=dom_id(business)%>">
        <td class="name"><%= business.name%></td>
    </tr>
    <%end%>
    </tbody>
</table>
